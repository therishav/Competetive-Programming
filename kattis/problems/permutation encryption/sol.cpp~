      cin >> temp;
      arr.push_back(temp);
    }

    vector<char> en(n+1);
    
    for(auto i : arr) 
      cout << i << " ";
    cout << '\n';
    
    cin.ignore(); 
    getline(cin, str);
    // cin >> str;
    while((str.size() % n) != 0)
      str.push_back('*');

    for(int i = 0; i < str.size(); i+=n) {
      for(int j = i; j <= n; j++) {
	en
    
    cout << "s: " << str << '\n';
  }
}
