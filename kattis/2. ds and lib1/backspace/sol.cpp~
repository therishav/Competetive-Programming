#include<bits/stdc++.h>
using namespace std;

int main(void) {
  string str, s="";
  cin >> str;

  for(int i = 0; i < str.size(); i++) {
    if(str[i] == '<') 
      erase(str.begin(), 2);
    else {
      s += str.front();
      erase(str.begin());
    }
  }
  
  for(auto i : sol)
    cout << i;
  cout << '\n';
}
