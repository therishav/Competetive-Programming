#include<bits/stdc++.h>
using namespace std;

int main(void) {
  int n;
  string str;

  while (true) {
    cin >> n;
    cin.ignore();
    if (n == 0)
      break;

    vector<string> arr;
    map<int, vector<int>> mp1;
    int hash, c;
    
    for (int i = 1; i <= n; i++) {
      getline(cin, str);
      cout << str << '\n';
      arr.push_back(str);
      
      hash = 0;
      for (int j = 1; j < str.size(); j++) 
	hash ^= (int)str[j];

      mp1[hash].push_back(i);

      cout << "keys: " << '\t' << "values: " << '\n';
      for (auto it = mp1.begin(); it != mp1.end(); it++) {
	cout << it->first << '\t';
	for (auto it2 = it->second.begin(); it2 != it->second.end(); it2++)
	  cout << *it2 << " ";
	cout << '\n';
      }
    }
    // int unique = 0, collision = 0;
    // for (auto it = mp1.begin(); it != mp1.end(); it++) {
    //   if (it->second.size() == 1)
	
    // }
  }
}
